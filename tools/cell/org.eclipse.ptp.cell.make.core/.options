# Debugging options for the org.eclipse.ptp.cell.make.core plugin

# Turn on general debugging for the org.eclipse.ptp.cell.make.core plugin.
# Also displays overall startup time.
org.eclipse.ptp.cell.make.core/debug = false

# Turn on tracing on xl provider.
org.eclipse.ptp.cell.make.core/debug/xlprovider = false

# String that is displayed before each trace message
org.eclipse.ptp.cell.make.core/trace/id = make.core

# Displays the thread ID on debug message
org.eclipse.ptp.cell.make.core/trace/thread = True

# Displays the timestamp on debug message
org.eclipse.ptp.cell.make.core/trace/timestamp = True

# Re-reads the .options file before executing a block of code that contains
# tracing information
org.eclipse.ptp.cell.make.core/trace/dynamic = False
