# Debugging options for the org.eclipse.ptp.cell.debug.be.ui plugin

# Turns on general debugging for the org.eclipse.ptp.cell.debug.be.ui plugin.
# Also displays overall startup time.
org.eclipse.ptp.cell.debug.be.ui/debug = false

# Turns on general debugging of toolchain searcher.
org.eclipse.ptp.cell.debug.be.ui/debug/searcher= false

# String that is displayed before each trace message
org.eclipse.ptp.cell.debug.be.ui/trace/id = be.ui

# Displays the thread ID on debug message
org.eclipse.ptp.cell.debug.be.ui/trace/thread = True

# Displays the timestamp on debug message
org.eclipse.ptp.cell.debug.be.ui/trace/timestamp = True

# Re-reads the .options file before executing a block of code that contains
# tracing information
org.eclipse.ptp.cell.debug.be.ui/trace/dynamic = False
