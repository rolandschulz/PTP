# Debugging options for the org.eclipse.ptp.cell.managedbuilder.gnu.ui plugin

# Turns on general debugging for the org.eclipse.ptp.cell.managedbuilder.gnu.ui plugin.
# Also displays overall startup time.
org.eclipse.ptp.cell.managedbuilder.gnu.ui/debug = false

# Turns on general debugging of option handlers.
org.eclipse.ptp.cell.managedbuilder.gnu.ui/debug/handler = false

# Turns on general debugging of toolchain support.
org.eclipse.ptp.cell.managedbuilder.gnu.ui/debug/support = false

# String that is displayed before each trace message
org.eclipse.ptp.cell.managedbuilder.gnu.ui/trace/id = gnu.ui

# Displays the thread ID on debug message
org.eclipse.ptp.cell.managedbuilder.gnu.ui/trace/thread = True

# Displays the timestamp on debug message
org.eclipse.ptp.cell.managedbuilder.gnu.ui/trace/timestamp = True

# Re-reads the .options file before executing a block of code that contains
# tracing information
org.eclipse.ptp.cell.managedbuilder.gnu.ui/trace/dynamic = False
