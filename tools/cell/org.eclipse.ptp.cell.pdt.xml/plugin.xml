            name="Configuration File For x86"
            project="false">
         <description>
            This wizard creates a XML configuration file for the PDT tool using x86 as the target architecture
         </description>
      </wizard> -->
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.eclipse.ptp.cell.pdt.xml.preferences.ui.PdtPreferenceInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="PDT"
            class="org.eclipse.ptp.cell.pdt.xml.preferences.ui.PdtPreferencesPage"
            id="org.eclipse.ptp.cell.pdt.xml.preferences.ui.PdtPreferencesPage"
            name="%PDTPreferencePageName">
      </page>
   </extension>

</plugin>
