<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.ptp.cell.pdt.xml.PdtXmlCategory"
            name="%PDTCategoryWizardName">
      </category>
      <wizard
            canFinishEarly="false"
            category="org.eclipse.ptp.cell.pdt.xml.PdtXmlCategory"
            class="org.eclipse.ptp.cell.pdt.xml.wizard.CellPdtXmlWizard"
            hasPages="true"
            id="org.eclipse.ptp.cell.pdt.xml.wizard.cell"
            name="%PDTConfigCellWizardName"
            project="false">
         <description>
         	%PDTConfigCellWizardDescription
         </description>
      </wizard>
