<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.ptp.debug.core"
   name="PTP Debug Core Plug-in"
   version="1.0.0"
   provider-name=""
   class="org.eclipse.ptp.debug.core.PTPDebugCorePlugin">

   <runtime>
      <library name="ptpdebugcore.jar">
         <export name="org.eclipse.ptp.debug.core.*"/>
         <export name="org.eclipse.ptp.debug.core.cdi.*"/>
         <export name="org.eclipse.ptp.debug.core.cdi.model.*"/>
         <export name="org.eclipse.ptp.debug.core.model.*"/>
         <export name="org.eclipse.ptp.debug.core.cdi.event.*"/>
      </library>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.cdt.core"/>
      <import plugin="org.eclipse.cdt.debug.core"/>
      <import plugin="org.eclipse.ptp.core"/>
   </requires>
   
   <extension-point id="PTPDebugger" name="Parallel Tools Core Debugger Extension" schema="schema/PTPDebugger.exsd"/>

</plugin>
