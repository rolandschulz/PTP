<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.ptp.debug.core"
   name="PTP Plug-in"
   version="1.0.0"
   provider-name="">

   <runtime>
      <library name="core.jar">
         <export name="*"/>
      </library>
   </runtime>
   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.cdt.core"/>
      <import plugin="org.eclipse.cdt.debug.core"/>
   </requires>
   <extension
         id="org.eclipse.ptp.debug.core.Debugger"
         point="org.eclipse.cdt.debug.core.CDebugger">
      <debugger
            class="org.eclipse.ptp.debug.core.PTPDebugger"
            cpu="native"
            id="org.eclipse.ptp.debug.core.Debugger"
            modes="run"
            name="%PTPDebugger.name"
            platform="*"/>
   </extension>

</plugin>
