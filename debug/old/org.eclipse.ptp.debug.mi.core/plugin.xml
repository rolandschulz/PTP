<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin
   id="org.eclipse.ptp.debug.mi.core"
   name="PTP Debug MI Core Plug-in"
   version="1.0.0"
   provider-name=""
   class="org.eclipse.ptp.debug.mi.core.PTPDebugMICorePlugin">

   <runtime>
      <library name="ptpdebugmicore.jar"/>
   </runtime>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.resources"/>
      <import plugin="org.eclipse.debug.core"/>
      <import plugin="org.eclipse.cdt.debug.core"/>
      <import plugin="org.eclipse.cdt.core"/>
      <import plugin="org.eclipse.ptp.debug.core"/>
   </requires>

   <extension
         point="org.eclipse.ptp.debug.core.PTPDebugger">
      <debugger
            platform="*"
            name="PTP"
            modes="run"
            cpu="native"
            class="org.eclipse.ptp.debug.mi.core.PTPDebugger"
            id="org.eclipse.ptp.debug.mi.core.PTPDebugger">
      </debugger>
   </extension>
   
</plugin>
