<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.0"?>
<plugin>
    
<!-- create perspective //-->
	<extension
		point="org.eclipse.ui.perspectives">
		<perspective
			name="%DebugParallelPerspective.name"
			icon="icons/ptp.gif"
			class="org.eclipse.ptp.debug.ui.PTPPerspectiveFactory"
			id="org.eclipse.ptp.debug.ui.ptpPerspectiveFactory"/>
	</extension>   

	<extension
		point="org.eclipse.ui.views">
		<category
			name="%DebugParallelViewCategory.name"
			id="org.eclipse.ptp.debug.ui.views">
		</category>
		<view
			name="%DebugParallelView.name"
			icon="icons/ptp.gif"
			category="org.eclipse.ptp.debug.ui.views"
			class="org.eclipse.ptp.debug.ui.views.DebugParallelView"
			id="org.eclipse.ptp.debug.ui.views.debugParallelView">
		</view>
	</extension>
