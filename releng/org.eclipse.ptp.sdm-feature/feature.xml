<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.ptp.debug.sdm"
      label="%featureName"
      version="7.0.0.qualifier"
      provider-name="%providerName"
      plugin="org.eclipse.ptp.debug.sdm.core">

   <description url="http://eclipse.org/ptp">
      %description
   </description>

   <copyright>
      %copyright
