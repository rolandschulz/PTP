<?xml version="1.0" encoding="UTF-8"?>
<feature
      id="org.eclipse.ptp.remote.rse"
      label="%featureName"
      version="3.0.0.qualifier"
      provider-name="%providerName"
      plugin="org.eclipse.ptp.remote.rse.core"
      image="eclipse_update_120.jpg">

   <description url="http://eclipse.org/ptp">
      %description
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="%updateSiteName" url="http://download.eclipse.org/tools/ptp/releases/3.0"/>
   </url>

   <requires>
      <import plugin="org.eclipse.core.runtime"/>
      <import plugin="org.eclipse.core.filesystem"/>
      <import plugin="org.eclipse.rse.core"/>
      <import plugin="org.eclipse.rse.subsystems.files.core"/>
      <import plugin="org.eclipse.rse.services"/>
      <import plugin="org.eclipse.rse.subsystems.shells.core"/>
      <import plugin="org.eclipse.ui"/>
      <import plugin="org.eclipse.ptp.remote.core"/>
      <import plugin="org.eclipse.ptp.remote.rse.core" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ptp.remote.core" version="2.0.1" match="greaterOrEqual"/>
      <import plugin="org.eclipse.ptp.remote.ui" version="2.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.rse.core" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.rse.files.ui" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.rse.subsystems.files.core" version="3.0.0" match="greaterOrEqual"/>
      <import plugin="org.eclipse.rse.ui" version="3.0.0" match="greaterOrEqual"/>
   </requires>

   <plugin
         id="org.eclipse.ptp.remote.rse.core"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

   <plugin
         id="org.eclipse.ptp.remote.rse.ui"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
