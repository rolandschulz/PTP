<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension-point id="synchronizeProviders" name="Synchronize Provider" schema="schema/synchronizeProviders.exsd"/>
   <extension
         id="remoteMakeNature"
         name="%RemoteMakeNature.name"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.eclipse.ptp.rdt.sync.core.resources.RemoteMakeNature">
         </run>
      </runtime>
      <builder
            id="org.eclipse.ptp.rdt.sync.core.remoteMakeBuilder">
      </builder>
   </extension>
   <extension
         id="org.eclipse.ptp.rdt.sync.core.remoteMakeBuilder"
         name="%RemoteMakeBuilder.name"
         point="org.eclipse.core.resources.builders">
      <builder
            callOnEmptyDelta="true"
            hasNature="true"
            isConfigurable="true">
         <run
               class="org.eclipse.ptp.rdt.sync.core.make.RemoteMakeBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="remoteSyncNature"
         name="%RemoteSyncNature.name"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="org.eclipse.ptp.rdt.sync.core.resources.RemoteSyncNature">
         </run>
      </runtime>
   </extension>
   <extension
         point="org.eclipse.ptp.services.core.services">
      <service
            id="org.eclipse.ptp.rdt.sync.core.SyncService"
            name="%SyncService.name">
      </service>
   </extension>
</plugin>
