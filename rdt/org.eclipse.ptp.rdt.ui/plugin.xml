        <category
              id="org.eclipse.ptp.rdt.core.category.cDevelopment"
              name="%CDevelopment.name">
        </category>
     </extension>
     <extension
           point="org.eclipse.ptp.services.core.services">
        <service
              categoryId="org.eclipse.ptp.rdt.core.category.cDevelopment"
              id="org.eclipse.ptp.rdt.core.CIndexingService"
              name="%CIndexingService.name"
              nullProviderClass="org.eclipse.ptp.rdt.ui.serviceproviders.NullCIndexServiceProvider"
