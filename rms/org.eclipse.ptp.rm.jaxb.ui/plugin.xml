<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ptp.services.ui.providerContributors">
      <providerContributor
            class="org.eclipse.ptp.rm.jaxb.ui.JAXBProviderContributor"
            id="org.eclipse.ptp.rm.jaxb.JAXBServiceProvider">
      </providerContributor>
   </extension>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="org.eclipse.ptp.ui.views.resourceManagerView"
            targetID="org.eclipse.ptp.ui.views.resourceManagerView">
         <action
               class="org.eclipse.ptp.rm.jaxb.ui.actions.OpenResourceManagerEditor"
               helpContextId="Select a preset or custom configuration to edit"
               icon="icons/edit.gif"
               id="org.eclipse.ptp.rm.jaxb.openResourceManagerEditorViewAction"
               label="Open Resource Manager Configuration Editor"
               menubarPath="editor"
               style="push"
               toolbarPath="editor"
               tooltip="Select a preset or custom configuration to edit">
         </action>
         <action
               class="org.eclipse.ptp.rm.jaxb.ui.actions.HideResourceManagerEditor"
               helpContextId="Hide the editor area"
               icon="icons/stop.png"
               id="org.eclipse.ptp.rm.jaxb.hideResourceManagerEditorViewAction"
               label="Hide Resource Manager Configuration Editor"
               menubarPath="editor"
               style="push"
               toolbarPath="editor"
               tooltip="Hide the editor area">
         </action>
      </viewContribution>
   </extension>

</plugin>
