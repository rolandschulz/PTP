<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.ptp.ui.preferences.ptpPreferencesPage"
            class="org.eclipse.ptp.simulation.ui.preferences.SimulationPreferencePage"
            id="org.eclipse.ptp.simulation.ui.preferences.SimulationPreferencePage"
            name="%SimulationPreferencePage.name">
      </page>
   </extension>
   <extension
         point="org.eclipse.ptp.ui.rmconfiguration">
      <configurationWizard
            class="org.eclipse.ptp.simulation.ui.wizards.SimulationRMConfigurationWizardPageFactory"
            id="org.eclipse.ptp.simulation.ui.configurationWizard"/>
   </extension>
   <extension
         point="org.eclipse.ptp.launch.rmLaunchConfiguration">
      <launchConfiguration
            class="org.eclipse.ptp.simulation.ui.rmLaunchConfiguration.SimulationRMLaunchConfigurationFactory"
            id="org.eclipse.ptp.simulation.ui.launchConfiguration"/>
   </extension>

</plugin>
