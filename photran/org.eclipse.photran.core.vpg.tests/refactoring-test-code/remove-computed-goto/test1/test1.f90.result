program test1
    implicit none

    integer :: n

    n = 1

    select case (n)
    case (1)
        go to 10
    case (2)
        go to 20
    case (3)
        go to 30
    case (4)
        go to 40
    end select

    print *, "It wasn't handled"
    stop

10  print *, "It was one"
    stop

