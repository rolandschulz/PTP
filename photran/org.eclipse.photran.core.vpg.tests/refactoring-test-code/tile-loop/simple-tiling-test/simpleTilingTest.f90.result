program test

    implicit none
    integer :: i1, j1

    integer :: i, j
    integer, dimension(50,60) :: A
    do i1=-15,45,20
        do j1=-15,45,20
            do i=max(1,i1),min(50,i1+19)
                do j=max(1,j1),min(60,j1+19)
                    A(i,j) = i+j
                end do
            end do
        end do
    end do


end program test
