         point="org.eclipse.rephraserengine.ui.vpg.vpg">
      <vpg
         id="org.eclipse.photran.ui.vpg"
         name="Fortran"
         class="org.eclipse.photran.internal.ui.vpg.PhotranVPGContribution" />
   </extension>

