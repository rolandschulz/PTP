      <category
            id="org.eclipse.photran.ui.views"
            name="Fortran Views">
      </category>
      
      <!-- Declaration View -->
      <view
            category="org.eclipse.photran.ui.views"
            class="org.eclipse.photran.internal.ui.views.declaration.DeclarationView"
