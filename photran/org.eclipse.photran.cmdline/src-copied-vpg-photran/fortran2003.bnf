(superclass):<DerivedTypeBodyConstruct> ::=
  | <PrivateSequenceStmt>
  | <ComponentDefStmt>

## R423
#<DerivedTypeStmt> ::=
#  | (inline):<LblDef> -:T_TYPE                                            (inline):<TypeName> -:T_EOS
#  | (inline):<LblDef> -:T_TYPE                        -:T_COLON -:T_COLON (inline):<TypeName> -:T_EOS
