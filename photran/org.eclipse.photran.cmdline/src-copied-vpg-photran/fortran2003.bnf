  | is-abstract(bool):T_ABSTRACT
  |     is-bind(bool):T_BIND    -:T_LPAREN language:T_IDENT -:T_RPAREN

<TypeParamName> ::= type-param-name:T_IDENT

# R424
<PrivateSequenceStmt> ::=
