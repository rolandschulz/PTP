# R211
(superclass):<InternalSubprogram> ::=
  | <FunctionSubprogram>
  | <SubroutineSubprogram>

# R212 doesn't ensure ordering as standard requires
(superclass):<ModuleSubprogramPartConstruct> ::=
  | <ContainsStmt>
  | <ModuleSubprogram>

# R213
(superclass):<ModuleSubprogram> ::=
  | <FunctionSubprogram>
  | <SubroutineSubprogram>

# R214
(superclass):<SpecificationStmt> ::=
  | <AccessStmt>
